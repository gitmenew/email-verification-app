# .env.example

# Cloudflare Turnstile secret key
CLOUDFLARE_SECRET=your_cloudflare_secret_key_here

# Base URL for redirecting users (your frontend app URL)
REDIRECT_BASE=https://your-frontend-url.up.railway.app

# Base URL for backend (used in token redirect logic)
BACKEND_BASE=https://your-backend-url.up.railway.app

# Enable debug logging (true or false)
DEBUG=false
